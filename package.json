{
  "name": "redux-saga-testing",
  "version": "0.0.5",
  "description": "A no-brainer way of testing your Sagas",
  "main": "main.js",
  "scripts": {
    "test": "cross-env jest && mocha ./mocha --compilers js:babel-core/register --require babel-polyfill",
    "test-jest": "cross-env jest",
    "test-mocha": "cross-env mocha ./mocha --compilers js:babel-core/register --require babel-polyfill",
    "test-jest-watch": "cross-env jest --watch",
    "test-mocha-watch": "cross-env mocha ./mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://antoinejaussoin@github.com/antoinejaussoin/redux-saga-testing.git"
  },
  "keywords": [
    "redux-saga",
    "sagas",
    "testing",
    "jest"
  ],
  "author": "Antoine Jaussoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antoinejaussoin/redux-saga-testing/issues"
  },
  "homepage": "https://github.com/antoinejaussoin/redux-saga-testing#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "jest": "^16.0.1",
    "mocha": "^3.1.2",
    "redux-saga": "^0.11.0",
    "sinon": "^1.17.6"
  },
  "dependencies": {},
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "rootDir": "jest"
  }
}
